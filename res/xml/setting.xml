<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
	
	
<CheckBoxPreference
	android:key="hideapp"
	android:title="Hide the app"
	android:summary="Hide the app frome the App Drawer"
	android:defaultValue="false">
</CheckBoxPreference>

	<EditTextPreference
		android:key="dialer"
		android:title="Dialer number"
		android:summary="Open the app with *#number#"
		android:inputType="number"
		android:defaultValue="1234"
		>
	</EditTextPreference>


</PreferenceScreen>
